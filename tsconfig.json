{
  "compilerOptions": {
    "target": "es2020",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": ["*"]
    }
  },
    "include": [
      "next-env.d.ts",
      "**/*.ts",
      "**/*.tsx",
      "**/*.js",
      "**/*.jsx"
, "app/api/upload_file/routetsx"    ],
  "exclude": ["node_modules", ".next", "out"]
}
